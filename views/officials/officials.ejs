                                <div>
                                    <div class="inputWithLabel" id="surubadan">
                                        <label>Last Name</label>
                                        <input type="text" aria-label="Last Name" name="punongBarangayLastName" required>
                                    </div>
                                    <div class="inputWithLabel">
                                        <label>First Name</label>
                                        <input type="text" aria-label="First Name" name="punongBarangayFirstName"
